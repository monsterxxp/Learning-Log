<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smallking.dao.SysUserDao">

    <resultMap id="sysUserResultMap" type="com.smallking.model.SysUser"></resultMap>

    <sql id="findDtoSql">
        select * from (
        select temp.*
        from tm_project_equipment_resource temp
        ) t
    </sql>
    <select id="findAll" resultMap="sysUserResultMap">
        select * from sys_user
    </select>
</mapper>